{
	"source": {
		"include": ["index.js", "lib", "README.md"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"opts": {
		"encoding": "utf8",
		"readme": "./README.md",
		"recurse": true,
		"verbose": true,
		"template": "node_modules/braintree-jsdoc-template",
		"destination": "./docs/"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"referenceTitle": "Node Twitch Reference"
	}
}